
CREATE TABLE liquidity
(
  liquidity character varying(128),
  asset_type character varying(128),
  instrument_type character varying(128),
  instrument_sub_type character varying(128)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE liquidity
  OWNER TO op;
GRANT ALL ON TABLE liquidity TO op;
GRANT SELECT ON TABLE liquidity TO opublic;

TRUNCATE TABLE  liquidity;

